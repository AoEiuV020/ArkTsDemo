{
  "name": "proto",
  "version": "1.0.0",
  "type": "module",
  "description": "proto",
  "main": "index.ts",
  "scripts": {
    "generate": "pbjs -t static-module -w es6 -o src/generated/user.js protos/user.proto && pbts -o src/generated/user.d.ts src/generated/user.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "ts-node": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "jest": "*",
    "protobufjs-cli": "1.1.1",
    "ts-jest": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@types/protobufjs": "^6.0.0",
    "long": "^5.3.2",
    "@ohos/protobufjs": "npm:protobufjs@7.2.4"
  }
}